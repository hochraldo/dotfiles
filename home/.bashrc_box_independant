# My bashrc which is box independant (work/home/...)


# Aliases
########

#Human readable figures
alias df='df -h'
alias du='du -h'

#directory listings
alias ll='ls -l'                              # long list
alias la='ls -A'                              # all but . and ..
alias l='ls -CF'

alias vi='vim'

alias g='git'
alias screen='screen -h 5000'
